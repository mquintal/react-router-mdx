{
  "name": "react-router-mdx",
  "type": "module",
  "version": "1.0.0",
  "main": "./dist/server/index.cjs",
  "module": "./dist/server/index.js",
  "scripts": {
    "test": "npm run test:server && npm run test:client",
    "test:watch": "jest --watch",
    "test:client": "echo \"not implemented\"",
    "test:server": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config jest.server.js",
    "lint": "eslint . --ext .ts,.tsx",
    "build:client": "vite build",
    "build:server": "tsup --tsconfig tsconfig.server.json",
    "build": "npm run build:client && npm run build:server",
    "prepare": "husky",
    "patch": "npm version patch && git push --follow-tags"
  },
  "keywords": [
    "react",
    "react-router",
    "md",
    "mdx"
  ],
  "author": "Miguel Quintal <mquintal88@gmail.com>",
  "license": "ISC",
  "description": "Simplifies the use of MDX files on a React Router v7 project.",
  "peerDependencies": {
    "@react-router/dev": "^7.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@jest/globals": "^29.7.0",
    "@react-router/dev": "^7.6.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.2",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "tslint-config-prettier": "^1.18.0",
    "tsup": "^8.5.0",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "exports": {
    "./client": {
      "import": "./dist/client/client.js",
      "types": "./dist/types/client/index.d.ts"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.cjs",
      "types": "./dist/server/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@mdx-js/mdx": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "glob": "^11.0.2",
    "gray-matter": "^4.0.3",
    "remark-frontmatter": "^5.0.0",
    "slash": "^5.1.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
